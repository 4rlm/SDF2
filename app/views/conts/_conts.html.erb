<%= render 'shared/conts_sidebar_btn' %>

<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-md-3">
    <h5><%= icon('fas', 'users', 'Contacts') %> <span style="font-size: 14px;">(Results: <%= number_with_delimiter(conts.count, :delimiter => ',') %> )</span></h5>
  </div>

  <div class="col-md-3">
    <%= render 'conts/download_conts', conts: conts %>
  </div>

  <div class="col-md-6"></div>
</div>



<div class="row">
  <% conts&.each do |cont| %>
    <div class="col-md-12">
      <%= render 'conts/cont_detail', cont: cont %>
    </div>
  <% end %>
</div>



<div class="row">
  <div class="col-md-4">
  </div>
  <div class="col-md-8 text-center">
    <%= will_paginate conts %>
  </div>
</div>
