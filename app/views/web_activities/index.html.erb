<p id="notice"><%= notice %></p>

<h1>Web Activities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @web_activities.each do |web_activity| %>
      <tr>
        <td><%= link_to 'Show', web_activity %></td>
        <td><%= link_to 'Edit', edit_web_activity_path(web_activity) %></td>
        <td><%= link_to 'Destroy', web_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Web Activity', new_web_activity_path %>
