<%= render partial: 'shared/webs_sidebar_btn' %>

<p id="notice"><%= notice %></p>

<div class="row">
  <div class="col-md-3">
    <h5>Websites <span style="font-size: 14px;">(Results: <%= number_with_delimiter(@webs.count, :delimiter => ',') %> )</span></h5>
  </div>


  <div class="col-md-3">
    <%= render 'webs/download_webs', webs: @webs %>
  </div>

  <div class="col-md-6"></div>
</div>



<div class="panel outer-panel">
  <% @webs.each do |web| %>

    <% @web = web %>
    <%= render 'webs/web_details', web: @web %>

  <% end %>

  <br>
  <%= will_paginate @webs %>
  <br>
  <br>
</div>
