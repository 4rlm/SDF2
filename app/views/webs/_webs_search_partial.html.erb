<div class="container">
  <% if @search.present? %>
    <%= search_form_for(@search, :html => {:class => "form"}) do |f| %>
    <% temps = @webs.select(:temp_name).where.not(temp_name: nil).pluck(:temp_name).uniq.sort %>


      <% temps.each do |temp_name| %>
        <%= check_box_tag('q[temp_name_in][]', temp_name) %>
        <%= temp_name %>
      <% end %>


      <div class="actions"><%= f.submit "Search" %></div>
      <div class=""><%= link_to "Clear Search", request.path, class:"cancel-button" %></div>
    <% end %>
  <% end %>
</div>





<div class="container">
  <form>

    <div class="form-group row">
      <div class="form-check">
        <label class="form-check-label">
          <input class="form-check-input" type="checkbox"> Box 1
        </label>
      </div>
    </div>

    <div class="form-group row">
      <div class="form-check">
        <label class="form-check-label">
          <input class="form-check-input" type="checkbox"> Box 2
        </label>
      </div>
    </div>

    <div class="form-group row">
      <div class="offset-sm-2 col-sm-10">
        <button type="submit" class="btn btn-primary">Sign in</button>
      </div>
    </div>


  </form>
</div>
