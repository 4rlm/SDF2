<div class="media">
  <div class="media-left">
    <%#= link_to image_tag(photo.image.url(:thumb), class: 'media-object'), photo.image.url, target: '_blank' %>

    <table class="table">
      <thead>
        <tr>
          <th>File Name</th>
          <th>Type</th>
          <th>Size</th>
          <th>Updated</th>
        </tr>
      </thead>

      <tbody>
        <% photos.each do |member| %>
          <%= member.attr  %>
        <% end


        <tr>
          <td><%= photo.csv_file_name %></td>
          <td><%= photo.csv_content_type %></td>
          <td><%= photo.csv_file_size %></td>
          <td><%= photo.csv_updated_at %></td>
        </tr>


      </tbody>
    </table>
  </div>
</div>
