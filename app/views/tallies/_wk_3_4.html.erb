<br>
<h5 class='text-center'>Activity Report</h5>
<h6 class="text-center"><%= icon('fas', 'calendar-alt', '3 Weeks Ago') %> </h6>

<!-- ACTS SCOPES BEGIN -->
<h6 class=""><%= icon('fas', 'building', 'Accounts') %> </h6>

<div class="row">
  <div class="col-md-12">
    <table class="table">

      <!-- thead rendered as partial -->
      <%= render 'tallies/tally_thead' %>

      <tbody>
        <!-- ACTS SCOPES BEGIN -->
        <!-- Act created_at -->
        <%= render 'tallies/tally_acts', tally_hsh: {th: 'Created', mod_name: 'Act', tally_scope: 'created_between_wk_3_4'} %>

        <!-- Act adr_changed -->
        <%= render 'tallies/tally_acts', tally_hsh: {th: 'Address Changed', mod_name: 'Act', tally_scope: 'adr_changed_between_wk_3_4'} %>

        <!-- Act act_changed -->
        <%= render 'tallies/tally_acts', tally_hsh: {th: 'Profile Changed', mod_name: 'Act', tally_scope: 'act_changed_between_wk_3_4'} %>

        <!-- Act ax_date -->
        <%= render 'tallies/tally_acts', tally_hsh: {th: 'InActive', mod_name: 'Act', tally_scope: 'ax_date_between_wk_3_4'} %>
      </tbody>
    </table>
  </div>
</div>

<br>

<!-- CONTS SCOPES BEGIN -->
<h6 class=""><%= icon('fas', 'users', 'Contacts') %> </h6>

