<p id="notice"><%= notice %></p>

<h1>Rooftops</h1>

<table class="table table-bordered table-hover table-striped text-nowrap">
  <thead>
    <tr>
      <th>RT Name</th>
      <th>RT Scrape Date</th>
    </tr>
  </thead>

  <tbody>
    <% @acts.each do |act| %>
      <tr>
        <td><%= link_to act.act_name, act %></td>
        <td><%= act.act_gp_date.strftime('%x') %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Act', new_act_path %>
