<p id="notice"><%= notice %></p>

<h1>Cont Activities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cont_activities.each do |cont_activity| %>
      <tr>
        <td><%= link_to 'Show', cont_activity %></td>
        <td><%= link_to 'Edit', edit_cont_activity_path(cont_activity) %></td>
        <td><%= link_to 'Destroy', cont_activity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cont Activity', new_cont_activity_path %>
